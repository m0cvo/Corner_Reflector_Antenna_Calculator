<Window x:Class="Corner_Reflector_Antenna_Calculator.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Corner_Reflector_Antenna_Calculator"
        mc:Ignorable="d"
        Title="Corner Reflector Antenna" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="344" Margin="10,38,0,0" VerticalAlignment="Top" Width="462" Source="/CornerReflector.jpg"/>
        <Button Content="Close" HorizontalAlignment="Left" Margin="695,382,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click"/>
        <Label Content="Enter Frequency in Mhz:" HorizontalAlignment="Left" Margin="366,63,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Label Content="SIde length (l):" HorizontalAlignment="Left" Margin="366,97,0,0" VerticalAlignment="Top" FontSize="14" Width="158"/>
        <Label Content="Spacing to dipole (s):" HorizontalAlignment="Left" Margin="366,131,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.16,0.597" FontSize="14" Width="158"/>
        <Label Content="Angle between sides (a):" HorizontalAlignment="Left" Margin="362,165,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.081,0.107" FontSize="14" Width="162"/>

    </Grid>
</Window>
